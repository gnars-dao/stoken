{
  "name": "@pioneer-platform/network",
  "version": "6.3.88",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "npm": "npm i",
    "npm-update": "npm update",
    "test": "npm run build && node __tests__/test-module.js",
    "test-cli-export": "npm run build && node __tests__/reference-test-cli-export.js",
    "lint": "prettier --write '**/**/*.ts'",
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node __tests__node",
    "build": "tsc -p .",
    "prepublish": "npm run build",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "test-jest": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "build:watch": "onchange 'src/**/*.ts' -- npm run build",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map lib node_modules"
  },
  "dependencies": {
    "@pioneer-platform/binance-network": "latest",
    "@pioneer-platform/blockbook": "latest",
    "@pioneer-platform/cosmos-network": "latest",
    "@pioneer-platform/eth-network": "latest",
    "@pioneer-platform/loggerdog": "latest",
    "@pioneer-platform/pioneer-coincap": "latest",
    "@pioneer-platform/pioneer-coins": "latest",
    "@pioneer-platform/pioneer-eth-token-data": "latest",
    "@pioneer-platform/thor-network": "latest",
    "@pioneer-platform/utxo-crypto": "latest",
    "@pioneer-platform/utxo-network": "latest",
    "@types/google-protobuf": "^3.7.4",
    "bech32": "^1.1.4",
    "bitcoinjs-lib": "^5.2.0",
    "crypto-js": "^4.0.0",
    "ethereumjs-util": "^7.0.10",
    "fiosdk-offline": "^1.2.21",
    "fs-extra": "^9.0.1",
    "google-protobuf": "^3.15.8",
    "node-fetch": "^2.6.1",
    "open": "^7.3.1",
    "openapi-client-axios": "^3.7.8",
    "socket.io-client": "^2.4.0",
    "web3-utils": "^1.3.5"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.48",
    "@types/source-map-support": "^0.5.3",
    "jest": "^26.6.3",
    "onchange": "^7.1.0",
    "serve": "^11.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^25.4.0",
    "typescript": "^3.9.7"
  }
}
