  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.50",
    "@types/source-map-support": "^0.5.3",
    "jest": "^26.6.3",
    "onchange": "^7.1.0",
    "serve": "^11.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^25.4.0",
    "typescript": "^3.9.9"
