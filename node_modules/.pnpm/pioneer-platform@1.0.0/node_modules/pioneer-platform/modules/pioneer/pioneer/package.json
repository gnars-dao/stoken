    "@pioneer-platform/pioneer-coins": "^6.2.44",
    "@pioneer-platform/pioneer-eth-token-data": "latest",
    "@pioneer-platform/utxo-crypto": "latest",
    "@types/google-protobuf": "^3.15.0",
    "bchaddrjs": "^0.5.2",
    "bech32": "^1.1.4",
