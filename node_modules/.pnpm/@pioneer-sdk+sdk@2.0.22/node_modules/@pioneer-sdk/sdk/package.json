{
  "name": "@pioneer-sdk/sdk",
  "version": "2.0.22",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "npm": "npm i",
    "npm-update": "npm update",
    "test": "npm run build && node __tests__/test-module.js",
    "test-ts": "rm -R lib && npm run build && ts-node __tests__/test-module-ts.ts",
    "lint": "prettier --write '**/**/*.ts'",
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node __tests__node",
    "build": "tsc -p .",
    "build:watch": "npm run build && onchange 'src/**/*.ts' -- npm run build",
    "prepublish": "npm run build",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "test-jest": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map lib node_modules"
  },
  "dependencies": {
    "@coinmasters/api": "^3.0.3",
    "@coinmasters/core": "^4.0.3",
    "@coinmasters/helpers": "^4.0.2",
    "@coinmasters/tokens": "^3.0.1",
    "@coinmasters/toolbox-cosmos": "^4.0.4",
    "@coinmasters/toolbox-evm": "^4.0.4",
    "@coinmasters/toolbox-ripple": "4.0.1",
    "@coinmasters/toolbox-utxo": "^4.0.4",
    "@coinmasters/types": "^4.0.2",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@keplr-wallet/types": "^0.12.48",
    "@pioneer-platform/loggerdog": "latest",
    "@pioneer-platform/pioneer-caip": "^9.0.10",
    "@pioneer-platform/pioneer-client": "^9.0.19",
    "@pioneer-platform/pioneer-coins": "^9.0.4",
    "@types/elliptic": "^6.4.18",
    "@types/node": "^20.10.4",
    "onchange": "^7.1.0",
    "typescript": "^5.3.3"
  }
}
