#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/esrever@0.2.0/node_modules/esrever/bin/node_modules:/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/esrever@0.2.0/node_modules/esrever/node_modules:/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/esrever@0.2.0/node_modules:/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/esrever@0.2.0/node_modules/esrever/bin/node_modules:/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/esrever@0.2.0/node_modules/esrever/node_modules:/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/esrever@0.2.0/node_modules:/Users/web3warrior/Documents/Code/pioneer/skateapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esrever@0.2.0/node_modules/esrever/bin/esrever" "$@"
else
  exec node  "$basedir/../../../../../esrever@0.2.0/node_modules/esrever/bin/esrever" "$@"
fi
