{
  "name": "@pioneer-sdk/pioneer-react",
  "version": "0.0.25",
  "author": "highlander",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "yarn": "yarn",
    "dev": "vite",
    "build": "tsc && NODE_OPTIONS=--max-old-space-size=32096 vite build",
    "build:turbo": "tsc && NODE_OPTIONS=--max-old-space-size=32096 npx turbo build",
    "build:turbo:publish": "tsc && npx turbo build && npm publish",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "npx turbo lint",
    "type-check": "tsc",
    "type-check:turbo": "npx turbo type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepublishOnly": "npm run check-declarations",
    "check-declarations": "if [ ! -f ./dist/index.d.ts ]; then echo 'Error: index.d.ts not found'; exit 1; fi"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@coinmasters/types": "^1.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@fontsource/plus-jakarta-sans": "^4.5.11",
    "@pioneer-sdk/sdk": "latest",
    "@types/ramda": "0.28.23",
    "@types/uuid": "^9.0.5",
    "assert": "^2.1.0",
    "bip39": "^3.1.0",
    "dotenv": "^16.3.1",
    "events": "^3.3.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.11.0",
    "react-image": "^4.1.0",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.16.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "short-uuid": "^4.2.2",
    "stream-browserify": "^3.0.0",
    "swagger-client": "^3.23.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.0",
    "@types/node": "^18.18.5",
    "@types/react": "18.2.0",
    "@types/react-dom": "^18.2.13",
    "@types/react-helmet": "^6.1.7",
    "@vitejs/plugin-react": "^4.1.0",
    "commitlint": "^17.8.0",
    "eslint": "^8.51.0",
    "eslint-config-sznm": "^1.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "typescript": "4.9.5",
    "vite": "^4.4.11"
  },
  "packageManager": "pnpm@8.1.0"
}
